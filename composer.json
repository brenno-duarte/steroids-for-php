{
    "name": "brenno-duarte/steroids-for-php",
    "description": "Steroids for PHP is a component that enhances PHP's native functions, offering features that are not present in PHP Core",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PHPPeclPolyfill\\": "extensions/"
        },
        "files": [
            "autoload-functions.php"
        ]
    },
    "require": {
        "php": ">= 8.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "jetbrains/phpstorm-stubs": "^2024.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2"
    }
}
